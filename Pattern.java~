public class Pattern {
    public static void main(String[] args) {
        for (int i = 0; i < 5; i++) {
            System.out.println();
            for (int j = 5-i; j > 0; j--) {
                System.out.println("i " +i + " j "+j);
            }
        }

        for (int i = 0; i < 5; i++) {
            System.out.println();
            for (int j = 5-i; j > 0; j--) {
                System.out.print("*");
            }
        }
    }
}
